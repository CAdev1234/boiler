<div class="header">
    <div class="back"><a href="javascript:history.go(-1);"><img src="web_images/return_01.jpg" alt="" style="width:100%" /></a></div>
    <h2>@ViewBag.classTitle</h2>
    <div class="ss"><a href="/search"><img src="web_images/sj_sousuo_03.jpg" alt="" style="width:100%" /></a></div>
    <div class="menu"><img src="web_images/sj_mune_04.jpg" alt="" style="width:100%;vertical-align:text-top" /></div>
</div>
<!--导航-->
<div class="daoh">
    @Html.Raw(ViewData["mobi_nav"])
</div>
<script>
                $(".header .menu").click(function () {
                    $(".daoh").toggle();
                })
                $(".daoh .nLi").click(function () {
                    $(this).addClass("on").siblings().removeClass("on");
                    $(this).children(".sub").show().parents(".nLi").siblings().children(".sub").hide();
                    $(this).siblings().children(".sub").hide();
                    if ($(".daoh .nLi .sub").is(":visible")) {
                        $(".bg").show();
                    }
                    else if ($(".daoh .nLi .sub").is(":hidden")) {
                        $(".bg").hide();
                    }
                })
                $(".search img").click(function () {
                    $(".hide").toggle();
                })
</script>